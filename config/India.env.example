# India NPPA Pharma Sahi Daam Scraper Configuration
# Copy this file to India.env and configure as needed

# Scraper identification
SCRAPER_ID=India

# Browser settings
HEADLESS=false
WAIT_SECONDS=60

# Input file settings
INPUT_FILENAME=formulations.csv

# Output file settings
OUTPUT_FILENAME=medicine_details.csv
EXCEL_EXPORT=true

# Processing limits (0 = no limit)
MAX_FORMULATIONS=0
MAX_MEDICINES_PER_FORMULATION=50

# Retry settings
MAX_RETRIES=3
RETRY_DELAY=5
