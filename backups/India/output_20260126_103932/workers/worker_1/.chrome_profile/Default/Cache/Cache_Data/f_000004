<script>




	function printDiv(divName){
		var printContents = document.getElementById(divName).innerHTML;
		var originalContents = document.body.innerHTML;
		document.body.innerHTML = printContents;

		window.print();
		document.body.innerHTML = originalContents;

	}

	function generateXLSCommon(e, dataDivId) {
		 
		var printImg = document.getElementById("printImg").innerHTML;
		document.getElementById("printImg").innerHTML = "";
		window.open('data:application/vnd.ms-excel,'
				+ encodeURIComponent($("#" + dataDivId + "").html()));
		e.preventDefault();
		document.getElementById("printImg").innerHTML = printImg;

	}

	function back(){
		document.listForm.action="pharmaSahiDaamTransList";
		document.listForm.method="GET";
		document.listForm.submit();
		}


</script>
 <script>
    $(document).ready(function () {
        $.fn.dataTable.ext.order['mixed-content-pre'] = function (data) {
            var num = parseFloat(data.match(/[\d\.]+/)[0]);
            var text = data.replace(num, '');
            return [num, text];
        };

        $('#myDatatable').DataTable({
            columnDefs: [
                {
                    targets: 5, // Index of the "secound last td" (0-based index)
                    type: 'mixed-content',
                    	render: function (data, type, row) {
                            // Customize the rendering of the column
                            if (type === 'display') {
                                // Set the text color to red for column 2
                                return '<span style="color: red;">' + data + '</span>';
                            }
                            return data;
                        }
                        
                }
            ]
        });
    });
    </script>
